<h1>Inventory</h1>
<a href="insert">Insert</a>

<table>
    <tr>
        <td>id</td>
        <td>name</td>
        <td>qty</td>
        <td>amount</td>
    </tr>
    <% inventory.forEach(function(item) { %>
    <tr>
        <td><%= item.id %></td>
        <td><%= item.name %></td>
        <td><%= item.qty %></td>
        <td><%= item.amount %></td>
        <td>
            <a href="/delete?id=<%= item.id %>" onClick="return confirm('Are you sure?')">Delete</a>
            <a href="/edit?id=<%= item.id %>">Edit</a>
        </td>
    </tr>
    <% }); %>
</table>
